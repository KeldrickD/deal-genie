{
  "version": 2,
  "buildCommand": "SKIP_ESLINT=1 NEXT_TELEMETRY_DISABLED=1 next build",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "nextjs",
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next",
      "config": {
        "reinstallDependencies": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_ENABLE_GENIE_NET": "true",
    "SKIP_ESLINT": "1",
    "NEXT_TELEMETRY_DISABLED": "1"
  }
} 